            (function($) {
              $(function() {
				$("#immomakler-contactform-form").submit(function(e) {
				    e.preventDefault();
				    $.post('https://kelm-immobilien.de/', $("#immomakler-contactform-form").serialize(), function(data) {
					  $('#immomakler-contactform').html(data);
					});
				return false;
